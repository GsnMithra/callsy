{
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "server/*"
  ],
  "scripts": {
    "build": "turbo build --no-daemon",
    "dev": "turbo dev --no-daemon --parallel",
    "start:local": "turbo start:local --no-daemon --parallel",
    "lint": "turbo lint --no-daemon --parallel",
    "<workspace name>": "yarn workspace <workspace name>",
    "web": "yarn workspace web",
    "docs": "yarn workspace docs",
    "utils": "yarn workspace utils",
    "ui": "yarn workspace ui"
  },
  "devDependencies": {
    "eslint": "8.48.0",
    "eslint-config-custom": "*",
    "prettier": "2.5.1",
    "turbo": "^2.0.11"
  },
  "packageManager": "yarn@1.22.19",
  "name": "callsy"
}
